{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/index.js"
    }
  ],
  "build": {
    "env": {
      "PRISMA_GENERATE": "true"
    }
  },
  "install": {
    "commands": [
      "prisma generate"
    ]
  }
}